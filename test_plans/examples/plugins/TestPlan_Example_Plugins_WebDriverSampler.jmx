        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="jp@gc - WebDriver Sampler" enabled="true">
          <stringProp name="WebDriverSampler.script">WDS.sampleResult.sampleStart()

WDS.log.info(&quot;TEST IS STARTED&quot;)

WDS.browser.get(&apos;${BASE_URL}&apos;)

var pkg = JavaImporter(org.openqa.selenium)
var lnkLogin = WDS.browser.findElement(pkg.By.id(&apos;header_login_link&apos;))


lnkLogin.click()


var txtLogin = WDS.browser.findElement(pkg.By.id(&apos;login_email&apos;))
txtLogin.sendKeys([&apos;${USERNAME}&apos;])

var txtPassword = WDS.browser.findElement(pkg.By.id(&apos;login_password&apos;))
txtPassword.sendKeys([&apos;${PASSWORD}&apos;])

var btnlogin = WDS.browser.findElement(pkg.By.xpath(&apos;//button[text()=&quot;Login&quot;]&apos;))
btnlogin.click()

var ui=JavaImporter(org.openqa.selenium.support.ui)
var wait = new ui.WebDriverWait(WDS.browser, 30)
wait.until(ui.ExpectedConditions.presenceOfElementLocated(pkg.By.xpath(&apos;//h1[contains(text(),&quot;welcome&quot;)]&apos;)))

var headingWelcome = WDS.browser.findElement(pkg.By.xpath(&apos;//h1[contains(text(),&quot;welcome&quot;)]&apos;))


if(headingWelcome.empty) {
    WDS.sampleResult.successful = false
    WDS.sampleResult.responseMessage = &apos;Could not login as a customer&apos;
}

WDS.sampleResult.sampleEnd()</stringProp>
